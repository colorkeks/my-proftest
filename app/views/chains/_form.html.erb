<%= simple_form_for @chain, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :test_id %>
  <%= f.input :section_id %>
  <%= f.input :eqvgroup_id %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                chains_path, :class => 'btn btn-default' %>
<% end %>
