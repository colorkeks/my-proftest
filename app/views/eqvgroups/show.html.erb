<%- model_class = Eqvgroup -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:test_id) %>:</strong></dt>
  <dd><%= @eqvgroup.test_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:section_id) %>:</strong></dt>
  <dd><%= @eqvgroup.section_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:number) %>:</strong></dt>
  <dd><%= @eqvgroup.number %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              eqvgroups_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_eqvgroup_path(@eqvgroup), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              eqvgroup_path(@eqvgroup),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
